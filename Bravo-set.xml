<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<!--
************************************************************************
Cessna 550 Citation-Bravo
Feb 2005  Syd Adams   ()
************************************************************************
-->
<PropertyList>
  <sim>
    <description>Cessna 550 Citation-Bravo w. 3d cockpit (YASim) </description>
    <author>Syd Adams</author>
    <status>beta</status>
    <flight-model>yasim</flight-model>
    <aero>Citation-Bravo-yasim</aero>
    <fuel-fraction>0.8</fuel-fraction>
    <startup>
      <splash-texture>Aircraft/Citation-Bravo/Bravo-splash.rgb</splash-texture>
    </startup>
    <panel>
      <path>Aircraft/737/Panels/737-ifr-panel.xml</path>
      <visibility>false</visibility>
    </panel>
    <model>
      <path>Aircraft/Citation-Bravo/Models/Bravo.xml</path>
    </model>
    <view n="0">
      <name>Cockpit</name>
      <type>lookfrom</type>
      <internal type="bool">true</internal>
      <config>
        <x-offset-m type="double">-0.27</x-offset-m>
        <y-offset-m type="double">1.0</y-offset-m>
        <z-offset-m type="double">3.18</z-offset-m>
        <pitch-offset-deg>-9</pitch-offset-deg>
        <default-field-of-view-deg type="double">60.0</default-field-of-view-deg>
      </config>
    </view>
    <view n="1">
      <config>
        <target-z-offset-m archive="y" type="double">6.7</target-z-offset-m>
      </config>
    </view>
    <view n="2">
      <config>
        <target-z-offset-m archive="y" type="double">6.7</target-z-offset-m>
      </config>
    </view>
    <view n="5">
      <config>
        <target-z-offset-m archive="y" type="double">6.7</target-z-offset-m>
      </config>
    </view>
    <systems>
      <electrical>
        <path>Aircraft/beech99/beech99-electrical.xml</path>
      </electrical>
      <autopilot>
        <path>Aircraft/Citation/Citation-II-autopilot.xml</path>
      </autopilot>
    </systems>
    <sound>
      <path>Aircraft/Citation/Citation-II-sound.xml</path>
    </sound>
    <hud>
      <enable3d>false</enable3d>
    </hud>
    <virtual-cockpit archive="y">true</virtual-cockpit>
  </sim>
  <controls>
    <gear>
      <brake-parking>1</brake-parking>
    </gear>
    <engines>
      <engine n="0">
        <magnetos>3</magnetos>
      </engine>
      <engine n="1">
        <magnetos>3</magnetos>
      </engine>
    </engines>
  </controls>
  <engines>
    <engine n="0">
      <rpm>1000</rpm>
      <reverser-position type="double">0.000000</reverser-position>
    </engine>
    <engine n="1">
      <rpm>1000</rpm>
      <reverser-position type="double">0.000000</reverser-position>
    </engine>
  </engines>
<!-- aircraft specific loading -->
  <yasim>
    <beech99>
      <pilot-lbs>400</pilot-lbs>
<!-- Only enable one of the following: -->
<!-- lightly loaded -->
      <passenger-lbs>100</passenger-lbs>
<!-- max load with 0.8 fuel fraction -->
<!-- <passenger-lbs>2178</passenger-lbs> -->
<!-- max load with 1.0 fuel fraction -->
<!-- <passenger-lbs>1878</passenger-lbs> -->
    </beech99>
  </yasim>
  <input>
    <keyboard>
      <key n="127">
        <name>Delete</name>
        <desc>Toggle Reversers</desc>
        <binding>
          <command>nasal</command>
          <script>citation.togglereverser()</script>
        </binding>
      </key>
    </keyboard>
  </input>
  <nasal>
    <citation>
      <file>Aircraft/Citation/Nasal/citation.nas</file>
    </citation>
  </nasal>
</PropertyList>