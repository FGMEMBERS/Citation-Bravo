    <animation>
        <type>select</type>
        <object-name>IAS-annun</object-name>
        <condition>
            <equals>
                    <property>autopilot/locks/altitude</property>
                    <value>IAS</value>
                </equals>
        </condition>
    </animation>

    <animation>
        <type>select</type>
        <object-name>MACH-annun</object-name>
        <condition>
            <equals>
                    <property>autopilot/locks/altitude</property>
                    <value>MACH</value>
                </equals>
        </condition>
    </animation>

    <animation>
        <type>select</type>
        <object-name>VSI-tgt</object-name>
        <condition>
            <equals>
                <property>autopilot/locks/altitude</property>
                <value>VS</value>
            </equals>
        </condition>
    </animation>

    <animation>
        <type>select</type>
        <object-name>Valt-display</object-name>
        <condition>
            <equals>
                <property>autopilot/locks/altitude</property>
                <value>VALT</value>
            </equals>
        </condition>
    </animation>

    <animation>
        <type>select</type>
        <object-name>Ralt</object-name>
        <condition>
            <less-than>
                <property>position/altitude-agl-ft</property>
                <property>autopilot/route-manager/min-lock-altitude-agl-ft</property>
            </less-than>
        </condition>
    </animation>

    <animation>
        <name>green</name>
        <type>material</type>
        <object-name>AP-status</object-name>
        <object-name>AP-lat</object-name>
        <object-name>AP-vert</object-name>
        <object-name>CRSannun</object-name>
        <object-name>HDGannun</object-name>
        <object-name>VSI-annun</object-name>
        <object-name>Ralt</object-name>
        <emission>
            <red>0.1</red>
            <green>0.9</green>
            <blue>0.1</blue>
        </emission>
        <ambient>
            <red>0</red>
            <green>0</green>
            <blue>0</blue>
        </ambient>
        <diffuse>
            <red>0</red>
            <green>0</green>
            <blue>0</blue>
        </diffuse>
        <specular>
            <red>0</red>
            <green>0</green>
            <blue>0</blue>
        </specular>
    </animation>

    <animation>
        <name>white</name>
        <type>material</type>
        <object-name>AP-lat-armed</object-name>
        <object-name>AP-vert-armed</object-name>
        <object-name>CRSlabel</object-name>
        <object-name>HDGlabel</object-name>
        <object-name>Mach</object-name>
        <emission>
            <red>0.9</red>
            <green>0.9</green>
            <blue>0.9</blue>
        </emission>
        <ambient>
            <red>0</red>
            <green>0</green>
            <blue>0</blue>
        </ambient>
        <diffuse>
            <red>0</red>
            <green>0</green>
            <blue>0</blue>
        </diffuse>
        <specular>
            <red>0</red>
            <green>0</green>
            <blue>0</blue>
        </specular>
    </animation>

    <animation>
        <name>magenta</name>
        <type>material</type>
        <object-name>ALT-annun</object-name>
        <object-name>ALT-min</object-name>
        <object-name>IAS-annun</object-name>
        <object-name>MACH-annun</object-name>
        <object-name>HDG-annun</object-name>
        <object-name>Valt-display</object-name>
        <object-name>Nav-src</object-name>
        <object-name>Nav-dst</object-name>
        <object-name>Nav-id</object-name>
        <emission>
            <red>0.95</red>
            <green>0.2</green>
            <blue>1.0</blue>
        </emission>
        <ambient>
            <red>0</red>
            <green>0</green>
            <blue>0</blue>
        </ambient>
        <diffuse>
            <red>0</red>
            <green>0</green>
            <blue>0</blue>
        </diffuse>
        <specular>
            <red>0</red>
            <green>0</green>
            <blue>0</blue>
        </specular>
    </animation>

    <animation>
