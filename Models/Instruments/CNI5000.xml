<?xml version="1.0"?>
<!--
Author : Syd Adams
CNI-5000 radio stack
-->
<PropertyList>

    <path>CNI5000.ac</path>

    <animation>
        <type>material</type>
        <object-name>CNI.btns</object-name>
        <emission>
            <red>0.025</red>
            <green>0.015</green>
            <blue>0.010</blue>
            <factor-prop>systems/electrical/outputs/instrument-lights</factor-prop>
        </emission>
    </animation>
<!--  KR-87-->
    <animation>
        <type>select</type>
        <object-name>CNI.adf</object-name>
        <condition>
            <property>instrumentation/kr-87/annunciators/adf</property>
        </condition>
    </animation>

    <animation>
        <type>select</type>
        <object-name>CNI.ant</object-name>
        <condition>
            <property>instrumentation/kr-87/annunciators/ant</property>
        </condition>
    </animation>

    <animation>
        <type>select</type>
        <object-name>CNI.bfo</object-name>
        <condition>
            <property>instrumentation/kr-87/annunciators/bfo</property>
        </condition>
    </animation>

    <animation>
        <type>select</type>
        <object-name>CNI.frq</object-name>
        <condition>
            <property>instrumentation/kr-87/annunciators/frq</property>
        </condition>
    </animation>

    <animation>
        <type>select</type>
        <object-name>CNI.flt</object-name>
        <condition>
            <property>instrumentation/kr-87/annunciators/flt</property>
        </condition>
    </animation>

    <animation>
        <type>select</type>
        <object-name>CNI.et</object-name>
        <condition>
            <property>instrumentation/kr-87/annunciators/et</property>
        </condition>
    </animation>

<!-- KT-70 unit 1-->

    <animation>
        <type>select</type>
        <object-name>CNI.alt1</object-name>
        <condition>
            <property>instrumentation/kt-70/annunciators/alt</property>
        </condition>
    </animation>

    <animation>
        <type>select</type>
        <object-name>CNI.fl1</object-name>
        <condition>
            <property>instrumentation/kt-70/annunciators/fl</property>
        </condition>
    </animation>

    <animation>
        <type>select</type>
        <object-name>CNI.gnd1</object-name>
        <condition>
            <property>instrumentation/kt-70/annunciators/gnd</property>
        </condition>
    </animation>

    <animation>
        <type>select</type>
        <object-name>CNI.on1</object-name>
        <condition>
            <property>instrumentation/kt-70/annunciators/on</property>
        </condition>
    </animation>

    <animation>
        <type>select</type>
        <object-name>CNI.R1</object-name>
        <condition>
            <property>instrumentation/kt-70/annunciators/reply</property>
        </condition>
    </animation>

    <animation>
        <type>select</type>
        <object-name>CNI.sby1</object-name>
        <condition>
            <property>instrumentation/kt-70/annunciators/sby</property>
        </condition>
    </animation>

    <animation>
        <type>rotate</type>
        <object-name>Xpdr1.knob</object-name>
        <property>instrumentation/kt-70/inputs/func-knob</property>
        <interpolation>
            <entry><ind>0</ind><dep>0.0</dep></entry>
            <entry><ind>1</ind><dep>30.0</dep></entry>
            <entry><ind>2</ind><dep>60.0</dep></entry>
            <entry><ind>3</ind><dep>90.0</dep></entry>
			<entry><ind>4</ind><dep>130.0</dep></entry>
			<entry><ind>5</ind><dep>160.0</dep></entry>
        </interpolation>
        <axis>
            <x>-1.0</x>
            <y>0.0</y>
            <z>0.0</z>
        </axis>
        <center>
            <x-m>0.0</x-m>
            <y-m>0.064</y-m>
            <z-m>-0.070</z-m>
        </center>
    </animation>

<!-- KT-70 unit 2-->

    <animation>
        <type>select</type>
        <object-name>CNI.alt2</object-name>
        <condition>
            <property>instrumentation/kt-70[1]/annunciators/alt</property>
        </condition>
    </animation>

    <animation>
        <type>select</type>
        <object-name>CNI.fl2</object-name>
        <condition>
            <property>instrumentation/kt-70[1]/annunciators/fl</property>
        </condition>
    </animation>

    <animation>
        <type>select</type>
        <object-name>CNI.gnd2</object-name>
        <condition>
            <property>instrumentation/kt-70[1]/annunciators/gnd</property>
        </condition>
    </animation>

    <animation>
        <type>select</type>
        <object-name>CNI.on2</object-name>
        <condition>
            <property>instrumentation/kt-70[1]/annunciators/on</property>
        </condition>
    </animation>

    <animation>
        <type>select</type>
        <object-name>CNI.R2</object-name>
        <condition>
            <property>instrumentation/kt-70[1]/annunciators/reply</property>
        </condition>
    </animation>

    <animation>
        <type>select</type>
        <object-name>CNI.sby2</object-name>
        <condition>
            <property>instrumentation/kt-70[1]/annunciators/sby</property>
        </condition>
    </animation>

    <animation>
        <type>rotate</type>
        <object-name>Xpdr2.knob</object-name>
        <property>instrumentation/kt-70[1]/inputs/func-knob</property>
        <interpolation>
            <entry><ind>0</ind><dep>0.0</dep></entry>
            <entry><ind>1</ind><dep>30.0</dep></entry>
            <entry><ind>2</ind><dep>60.0</dep></entry>
            <entry><ind>3</ind><dep>90.0</dep></entry>
			<entry><ind>4</ind><dep>130.0</dep></entry>
			<entry><ind>5</ind><dep>160.0</dep></entry>
        </interpolation>
        <axis>
            <x>-1.0</x>
            <y>0.0</y>
            <z>0.0</z>
        </axis>
        <center>
            <x-m>0.0</x-m>
            <y-m>0.064</y-m>
            <z-m>-0.105</z-m>
        </center>
    </animation>

</PropertyList>